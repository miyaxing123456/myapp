<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>Math.random()</title>
</head>
<body>
<script>
  //补充代码
  var ran=Math.floor(Math.random()*9+1);
  var num=prompt("请输入");
  if(num===ran){
      alert("恭喜你猜对了");
  }else{
      while(num!=ran){
      if(num<ran){
          alert("你猜小了");
          var num=prompt("请输入");
      }
      if(num>ran){
          alert("你猜大了");
          var num=prompt("请输入");
      }
   }
   alert("恭喜你猜对了");
  }
    
</script>
</body>
</html>
